{
  "latex-workshop.latex.recipes": [
    {
      "name": "build with externalized images",
      "tools": ["cleanstyles", "pdflatex", "pdflatex"]
    },
    {
      "name": "clean and build with externalized images",
      "tools": ["cleanfiles", "cleanstyles", "pdflatex", "pdflatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "cleanstyles",
      "command": "python3",
      "args": ["envsetup.py", "cleanstyle"],
      "env": {}
    },
    {
      "name": "cleanfiles",
      "command": "python3",
      "args": ["envsetup.py", "deletetemp"],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.recipe.default": "build with externalized images",
  "latex-workshop.latex.autoBuild.interval": 5000,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.message.error.show": false,
  "latex-workshop.view.outline.sections": [
    "part",
    "chapter",
    "section",
    "subsection",
    "subsubsection",
    "paragraph"
  ],
  "latex-workshop.latexindent.args": [
    "-c",
    "%DIR%/",
    "%TMPFILE%",
    "-y=defaultIndent: '%INDENT%'",
    "-m"
  ],
  "[latex]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": [
    "abovesep",
    "auxlock",
    "belowsep",
    "Breveglieri",
    "Brzozowski",
    "buildtree",
    "cleanstyle",
    "deletetemp",
    "determinization",
    "Determinize",
    "distinguishability",
    "DPDA",
    "Dyck",
    "elemid",
    "Greibach",
    "halign",
    "Kleene",
    "metasymbol",
    "metasymbols",
    "Morzenti",
    "Nivat",
    "nonambiguous",
    "nondeterministically",
    "nonleft",
    "nonnullable",
    "nonterminal",
    "nonterminals",
    "PCFG",
    "postaccessible",
    "powerset",
    "pushdown",
    "reentrant",
    "Reghizzi",
    "rulesep",
    "semidecidable",
    "Sethi",
    "SPPF",
    "subexpressions",
    "subjacent",
    "tblr",
    "terminalshift",
    "tikzit",
    "tikzstyle",
    "tikzstyles",
    "tikztemp",
    "tradeoff",
    "undistinguishability",
    "Unilinear",
    "valign",
    "Verlag",
    "vsms"
  ]
}
